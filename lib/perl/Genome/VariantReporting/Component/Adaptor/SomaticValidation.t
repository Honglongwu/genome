#!/usr/bin/env genome-perl

BEGIN { 
    $ENV{UR_DBI_NO_COMMIT} = 1;
    $ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;
}

use strict;
use warnings;

use above "Genome";
use Test::More;
use Sub::Install qw(reinstall_sub);
use Genome::VariantReporting::Component::Adaptor::TestHelpers qw(test_accessor setup_results);

use_ok("Genome::VariantReporting::Component::Adaptor");

my $build = Genome::Model::Build::SomaticValidation->__define__();
my ($bam_result1, $bam_result2, $snv_vcf_result, $indel_vcf_result) = setup_results();

reinstall_sub( {
    into => $build->class,
    as => 'control_merged_alignment_result',
    code => sub {my $self = shift;
        return $bam_result1;
    },
});
reinstall_sub( {
    into => $build->class,
    as => 'merged_alignment_result',
    code => sub {my $self = shift;
        return $bam_result2;
    },
});

test_accessor($build, $bam_result1, $bam_result2);

done_testing();
